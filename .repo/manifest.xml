<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- remote: name 名称，fetch 远程地址 -->
  <remote name="private"
          fetch="http://www.catchitime.com:8081/diffusion/"/>

  <remote name="github"
          fetch="https://github.com/yishuiliunian/"/>

  <!-- default: revision 指定分支，remote 指定remote name，sync-j 同步线程数 -->
  <default revision="master"
           remote="origin"
           sync-j="4" />

  <!-- project：path 下载后的本地路径，name 相对远程路径，完整路径为 remote.fetch + project.name -->
  <project remote="private" path="Libs/ElementKit" name="EK/elementkit.git" />
  <project remote="private" path="Core/ChatDailyProject" name="CDPROJECT/chatdailyproject.git" />
  <project remote="private" path="Core/ChatDailyCore" name="CDCORE/chatdailycore.git" />
  <project remote="github" path="Libs/EKInputExtention" name="EKInputExtention.git" />
  <project remote="github" path="Libs/DZChatUI" name="DZChatUI.git" />
  <project remote="github" path="Libs/DZProgrameDefines" name="DZProgrameDefines.git" />
  <project remote="github" path="Libs/YHMapLocation" name="YHMapLocation.git" />
  <project remote="github" path="Libs/DZAdjustFrame" name="DZAdjustFrame.git" />
  <project remote="github" path="Libs/DZGeometryTools" name="DZGeometryTools.git" />
  <project remote="github" path="Libs/DZKeyboardManager" name="DZKeyboardManager.git" />
  <project remote="github" path="Libs/DZMemo" name="DZMemo.git" />
  <project remote="github" path="Libs/StyleSheet" name="StyleSheet.git" />
  <project remote="github" path="Libs/MRLogicInjection" name="MRLogicInjection.git" />
  <project remote="github" path="Libs/YYText" name="YYText.git" />
  <project remote="github" path="Libs/DZAudio" name="DZAudio.git" />
  <project remote="github" path="Libs/DZCache" name="DZCache.git" />
</manifest>
